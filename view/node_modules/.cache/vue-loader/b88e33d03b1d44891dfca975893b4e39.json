CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"UpdateHouse.vue","sourceRoot":"src/views/service","sourcesContent":["<template>\r\n  <div class=\"house-container\">\r\n    <div class=\"nav\">\r\n      <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n        <el-breadcrumb-item style=\"cursor: pointer;\" @click.native=\"toLastPage\">\r\n          <span style=\"color: rgb(51,51,51);font-size: 18px;\">ÊàøÊ∫êÁÆ°ÁêÜ</span>\r\n        </el-breadcrumb-item>\r\n\r\n        <el-breadcrumb-item>\r\n          <span style=\"color: rgb(51,51,51);font-size: 18px;\"\r\n            >ÊàøÂ±ã‰ø°ÊÅØ‰øÆÊîπ</span\r\n          >\r\n        </el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n\r\n    <!-- ÊàøÂ±ã‰ø°ÊÅØÂå∫Âüü -->\r\n    <div class=\"update-area\">\r\n      <!-- ÊàøÂ±ãÂêçÁß∞ -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ãÂêç</span>\r\n\r\n        <el-input\r\n          style=\"width: 600px;\"\r\n          v-model=\"house.name\"\r\n          placeholder=\"ËØ∑ËæìÂÖ•Ôºå30‰∏™Â≠ó‰ª•ÂÜÖ\"\r\n        ></el-input>\r\n      </div>\r\n\r\n      <!-- ÁúÅ‰ªΩ‰ø°ÊÅØ -->\r\n      <div class=\"area\">\r\n        <span>ÊâÄÂ±û‰ΩçÁΩÆ</span>\r\n\r\n        <div style=\"display: flex;gap: 20px;\">\r\n          <el-select\r\n            @change=\"handleAreaChange\"\r\n            style=\"width: 290px;\"\r\n            v-model=\"topAreaId\"\r\n            placeholder=\"ËØ∑ÈÄâÊã©\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in topArea\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n\r\n          <el-select\r\n            @change=\"fetchCommunity\"\r\n            style=\"width: 290px;\"\r\n            v-model=\"cityAreaId\"\r\n            placeholder=\"ËØ∑ÈÄâÊã©\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in cityArea\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Â∞ÅÈù¢ -->\r\n      <div class=\"area\">\r\n        <span>Â∞ÅÈù¢</span>\r\n\r\n        <div class=\"user-avatar\">\r\n          <p style=\"font-size: 12px;color: rgb(0, 119, 184);\">\r\n            ÁÇπÂáªüì∑Â§ÑÂç≥ÂèØ‰∏ä‰º†ÊàøÂ±ãÂ∞ÅÈù¢\r\n          </p>\r\n\r\n          <img\r\n            v-if=\"cover\"\r\n            style=\"width: 290px;height: 170px;border-radius: 5px;\"\r\n            :src=\"cover || ''\"\r\n            alt=\"\"\r\n          />\r\n          <el-upload\r\n            class=\"avatar-uploader\"\r\n            action=\"file/upload\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleImageSuccess\"\r\n          >\r\n            <i class=\"el-icon-camera-solid\"></i>\r\n          </el-upload>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ÂÆûÂÜµÂõæ -->\r\n      <div class=\"area\">\r\n        <span>ÂÆûÂÜµÂõæ</span>\r\n\r\n        <div>\r\n          <el-upload\r\n            :file-list=\"coverList\"\r\n            :on-success=\"handleCovers\"\r\n            action=\"file/upload\"\r\n            list-type=\"picture-card\"\r\n            :on-preview=\"handlePictureCardPreview\"\r\n            :on-remove=\"handleRemove\"\r\n          >\r\n            <i class=\"el-icon-plus\"></i>\r\n          </el-upload>\r\n\r\n          <el-dialog :modal=\"false\" :visible.sync=\"dialogVisible\">\r\n            <img\r\n              style=\"z-index: 1000;\"\r\n              width=\"100%\"\r\n              :src=\"dialogImageUrl\"\r\n              alt=\"\"\r\n            />\r\n          </el-dialog>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ã‰ªãÁªç -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ã‰ªãÁªç</span>\r\n\r\n        <div style=\"background-color: rgba(35, 170, 242,0.2);padding: 1px;\">\r\n          <Editor\r\n            style=\"width: 600px;\"\r\n            :receiveContent=\"content\"\r\n            height=\"300px\"\r\n            api=\"file/upload\"\r\n            @on-listener=\"onListener\"\r\n          />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ÊâÄÂ±ûÂ∞èÂå∫ -->\r\n      <div class=\"area\">\r\n        <span>ÊâÄÂ±ûÂ∞èÂå∫</span>\r\n\r\n        <div style=\"display: flex;gap: 20px;\">\r\n          <el-select\r\n            @change=\"fetchCommunity\"\r\n            style=\"width: 290px;\"\r\n            v-model=\"house.communityId\"\r\n            placeholder=\"ËØ∑ÈÄâÊã©\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in communityList\"\r\n              :key=\"item.id\"\r\n              :label=\"item.name\"\r\n              :value=\"item.id\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãÁ±ªÂûã -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ãÁ±ªÂûã</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.typeId\">\r\n          <el-radio\r\n            v-for=\"item in houseTypeList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãÊúùÂêë -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ãÊúùÂêë</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.directionId\">\r\n          <el-radio\r\n            v-for=\"item in houseDirectionList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãÊà∑Âûã -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ãÊà∑Âûã</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.sizedId\">\r\n          <el-radio\r\n            v-for=\"item in houseSizedList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãÊäºÈáëÊñπÂºè -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ãÊäºÈáëÊñπÂºè</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.depositMethodId\">\r\n          <el-radio\r\n            v-for=\"item in houseDepositMethodList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãÊòØÂê¶‰∏¥ËøëÂú∞ÈìÅ -->\r\n      <div class=\"area\">\r\n        <span>ÊàøÂ±ãÊòØÂê¶‰∏¥ËøëÂú∞ÈìÅ</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.isSubway\">\r\n          <el-radio\r\n            v-for=\"item in houseSubwayList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <div class=\"area\" v-if=\"house.isSubway === 1\">\r\n        <span>Âú∞ÈìÅÁ∫øË∑Ø</span>\r\n\r\n        <el-slider\r\n          style=\"width: 300px;\"\r\n          :max=\"27\"\r\n          v-model=\"house.subwayLine\"\r\n          :step=\"1\"\r\n          show-stops\r\n        >\r\n        </el-slider>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãË£Ö‰øÆÁä∂ÊÄÅ -->\r\n      <div class=\"area\">\r\n        <span>Ë£Ö‰øÆÁä∂ÊÄÅ</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.fitmentStatusId\">\r\n          <el-radio\r\n            v-for=\"item in houseFitmentStatusList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <!-- ÊàøÂ±ãÁßüËµÅÊñπÂºè -->\r\n      <div class=\"area\">\r\n        <span>ÁßüËµÅÊñπÂºè</span>\r\n\r\n        <el-radio-group aria-hidden=\"false\" v-model=\"house.rentalType\">\r\n          <el-radio\r\n            v-for=\"item in houseRentalTypeList\"\r\n            :key=\"item.value\"\r\n            :label=\"item.value\"\r\n            >{{ item.label }}</el-radio\r\n          >\r\n        </el-radio-group>\r\n      </div>\r\n\r\n      <!-- ÁîüÊ¥ªËÆæÊñΩÈ°πÈÖçÁΩÆ -->\r\n      <div class=\"area\">\r\n        <span>ÁîüÊ¥ªËÆæÊñΩÈ°πÈÖçÁΩÆ</span>\r\n\r\n        <div class=\"living\">\r\n          <div\r\n            class=\"living-item\"\r\n            v-for=\"(item, index) in houseLivingFacilityList\"\r\n            :key=\"index\"\r\n          >\r\n            <div class=\"text\">{{ item.label }}</div>\r\n\r\n            <div>\r\n              <el-switch\r\n                v-model=\"item.selected\"\r\n                active-color=\"#13ce66\"\r\n                inactive-color=\"#929292\"\r\n              >\r\n              </el-switch>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!--‰∫ßÊùÉÈù¢ÁßØ-->\r\n      <div class=\"area\">\r\n        <span>‰∫ßÊùÉÈù¢ÁßØ</span>\r\n\r\n        <div>\r\n          <el-input\r\n            v-model=\"house.sizeNumber\"\r\n            placeholder=\"‰∫ßÊùÉÈù¢ÁßØ\"\r\n          ></el-input>\r\n        </div>\r\n      </div>\r\n\r\n      <!--ÊâÄÂ±ûÊ•ºÂ±Ç-->\r\n      <div class=\"area\">\r\n        <span>ÊâÄÂ±ûÊ•ºÂ±Ç</span>\r\n\r\n        <div style=\"display: flex;align-items: center;\">\r\n          <el-input\r\n            v-model=\"house.floor\"\r\n            placeholder=\"ËæìÂÖ•ÔºàÈ´ò/‰∏≠/‰ΩéÔºâ\"\r\n          ></el-input>\r\n        </div>\r\n      </div>\r\n\r\n      <!--ÊúàÁßüÈáë-->\r\n      <div class=\"area\">\r\n        <span>ÊúàÁßüÈáë</span>\r\n\r\n        <div>\r\n          <el-input v-model=\"house.rent\" placeholder=\"¬•ÁßüÈáëÔºàÂÖÉÔºâ\"></el-input>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"area\">\r\n        <div\r\n          class=\"info-bt\"\r\n          @click=\"updateHouse\"\r\n          style=\"text-align: center;width: 200px;margin-left: 130px;margin-top: 30px;\"\r\n        >\r\n          ‰øÆÊîπÊàøÊ∫ê‰ø°ÊÅØ\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Editor from \"@/components/Editor.vue\";\r\nimport Tab from \"@/components/Tab.vue\";\r\nexport default {\r\n  components: { Editor, Tab },\r\n  name: \"CommunityUpdate\",\r\n  data() {\r\n    return {\r\n      communityList: [], // Â∞èÂå∫Êï∞ÊçÆ\r\n      communityInfo: {},\r\n      house: {\r\n        typeId: null, // Á±ªÂûã\r\n        directionId: null, // ÊúùÂêë\r\n        sizedId: null, // Êà∑Âûã\r\n        depositMethodId: null, // ÁßüÈáëÊñπÂºè\r\n        isSubway: null, // ÊòØÂê¶‰∏¥ËøëÂú∞ÈìÅ\r\n        fitmentStatusId: null, // Ë£Ö‰øÆÁä∂ÊÄÅ\r\n        rentalType: null // ÁßüËµÅÊñπÂºè\r\n      },\r\n      name: null,\r\n      cover: \"\",\r\n      dialogImageUrl: \"\",\r\n      dialogVisible: false,\r\n      coverList: [],\r\n      topArea: [], //ÁúÅ‰ªΩ‰ø°ÊÅØ\r\n      cityArea: [], // Â∏ÇÂå∫‰ø°ÊÅØ\r\n      topAreaId: null, // ÁúÅ‰ªΩ‰ø°ÊÅØ\r\n      cityAreaId: null, // ÂüéÂ∏ÇID\r\n      content: \"\",\r\n      houseTypeList: [], // ÊàøÂ±ãÁ±ªÂûãÊï∞ÁªÑ\r\n      houseDirectionList: [], // ÊàøÂ±ãÊúùÂêëÊï∞ÁªÑ\r\n      houseSizedList: [], // ÊàøÂ±ãÊà∑ÂûãÊï∞ÁªÑ\r\n      houseDepositMethodList: [], // ÊàøÂ±ãÊäºÈáëÊñπÂºèÊï∞ÁªÑ\r\n      houseSubwayList: [], // ÊàøÂ±ã‰∏¥ËøëÂú∞ÈìÅÁä∂ÊÄÅ\r\n      houseFitmentStatusList: [], // ÊàøÂ±ãË£Ö‰øÆÁä∂ÊÄÅÊï∞ÁªÑ\r\n      houseRentalTypeList: [], // ÊàøÂ±ãÁßüËµÅÊñπÂºèÊï∞ÁªÑ\r\n      houseLivingFacilityList: [], // Êü•ËØ¢ÊàøÂ±ãÁîüÊ¥ªËÆæÊñΩÈÖçÁΩÆÈ°πÊï∞ÁªÑ\r\n      houseId: null\r\n    };\r\n  },\r\n  created() {\r\n    this.fetchTopArea();\r\n    this.fetchHouseType();\r\n    this.fetchHouseDirection();\r\n    this.fetchHouseSized();\r\n    this.fetchHouseDepositMethod();\r\n    this.fetchHouseSubway();\r\n    this.fetchHouseFitmentStatus();\r\n    this.fetchHouseRentalType();\r\n    this.fetchLivingFacilityType();\r\n    this.getPathId();\r\n  },\r\n  methods: {\r\n    // ÂèñÂæóË∑ØÂæÑ‰∏äÁöÑÊê∫Â∏¶ID\r\n    getPathId() {\r\n      this.houseId = this.$router.history.current.query.houseId;\r\n      this.fetchHouseInfo(this.houseId);\r\n    },\r\n    async fetchHouseInfo(id) {\r\n      try {\r\n        const { data } = await this.$axios.get(`/house/getById/${id}`);\r\n        this.topAreaId = data.topAreaId; // ËÆæÁΩÆÁúÅ‰ªΩ\r\n        this.handleAreaChange(this.topAreaId); // ÈÄöËøáÁúÅ‰ªΩIDÊü•ËØ¢‰∏ãÈù¢ÊâÄÊúâÁöÑÂ∏ÇÂå∫\r\n        this.cityAreaId = data.areaId; // ËÆæÁΩÆÂ∏ÇÂå∫\r\n        this.fetchCommunity(this.cityAreaId); // ÈÄöËøáÂ∏ÇÂå∫IDÊü•ËØ¢Â∫ï‰∏ãÂÖ®ÈÉ®ÁöÑÂ∞èÂå∫\r\n        // Á°Æ‰øùÊâÄÊúâIDÂ≠óÊÆµÊòØÊï∞ÂÄºÁ±ªÂûã\r\n        this.house = {\r\n          ...data,\r\n          typeId: Number(data.typeId),\r\n          directionId: Number(data.directionId),\r\n          sizedId: Number(data.sizedId),\r\n          depositMethodId: Number(data.depositMethodId),\r\n          fitmentStatusId: Number(data.fitmentStatusId),\r\n          rentalType: Number(data.rentalType),\r\n          isSubway: data.isSubway ? 1 : 2\r\n        };\r\n        this.cover = data.cover;\r\n        this.house.isSubway = data.isSubway ? 1 : 2;\r\n        console.log(data);\r\n        // Â§ÑÁêÜÂÆûÂÜµÂõæ\r\n        this.coverList = (data.covers || []) // Èò≤Ê≠¢ covers ‰∏∫ null/undefined\r\n          .split(\",\") // ÂÜçÊãÜÂàÜÊàêÊï∞ÁªÑÔºàÂ¶Ç [\"url1\", \"url2\"]Ôºâ\r\n          .filter(url => url.trim() !== \"\") // ËøáÊª§Á©∫Â≠óÁ¨¶‰∏≤\r\n          .map(url => ({\r\n            uid: Date.now() + Math.floor(Math.random() * 1000),\r\n            url: url.trim() // ÂéªÈô§ÂâçÂêéÁ©∫Ê†º\r\n          }));\r\n        // ËÆæÁΩÆÊàøÂ±ãËØ¶ÊÉÖ\r\n        this.content = data.detail;\r\n        // ËÆæÁΩÆÁîüÊ¥ªËÆæÊñΩ\r\n        this.houseLivingFacilityList = JSON.parse(data.livingFacilities);\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ã‰ø°ÊÅØÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // ÊàøÂ±ãÁßüËµÅÊñπÂºè\r\n    handleRentalTypeChange(obj) {\r\n      this.house.rentalType = Number(obj.value);\r\n    },\r\n    // ÊàøÂ±ãË£Ö‰øÆÁä∂ÊÄÅ\r\n    handleFitmentStatusChange(obj) {\r\n      this.house.fitmentStatusId = Number(obj.value);\r\n    },\r\n    // ÊàøÂ±ãÊòØÂê¶‰∏¥ËøëÂú∞ÈìÅÈÄâÊã©\r\n    handleSubwayChange(obj) {\r\n      this.house.isSubway = obj.value;\r\n    },\r\n    // ÊàøÂ±ãÁ±ªÂûãÈÄâÊã©\r\n    handleChange(obj) {\r\n      this.house.typeId = Number(obj.value);\r\n    },\r\n    // ÊúùÂêëÈÄâÊã©\r\n    handleDirectionChange(obj) {\r\n      this.house.directionId = Number(obj.value);\r\n    },\r\n    // Êà∑ÂûãÈÄâÊã©\r\n    handleSizedChange(obj) {\r\n      this.house.sizedId = Number(obj.value);\r\n    },\r\n    async fetchCommunity(cityAreaId) {\r\n      try {\r\n        const { data } = await this.$axios.post(\"/community/list\", {\r\n          areaId: cityAreaId\r\n        });\r\n        this.communityList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢Â∏ÇÂå∫‰∏ãÈù¢ÁöÑÂ∞èÂå∫‰ø°ÊÅØÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãÁ±ªÂûã\r\n    async fetchHouseType() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseTypeList\");\r\n        this.houseTypeList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÁ±ªÂûãÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãÊúùÂêë\r\n    async fetchHouseDirection() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseDirectionList\");\r\n        this.houseDirectionList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÊúùÂêëÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãÊà∑Âûã\r\n    async fetchHouseSized() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseSizedList\");\r\n        this.houseSizedList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÊà∑ÂûãÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãÊäºÈáëÊñπÂºèÊï∞ÁªÑ\r\n    async fetchHouseDepositMethod() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseDepositMethodList\");\r\n        this.houseDepositMethodList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÊà∑ÂûãÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ã‰∏¥ËøëÂú∞ÈìÅÊï∞ÁªÑ\r\n    async fetchHouseSubway() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseSubwayList\");\r\n        this.houseSubwayList = data;\r\n        console.log(this.houseSubwayList);\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÊòØÂê¶‰∏¥ËøëÂú∞ÈìÅÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãË£Ö‰øÆÁä∂ÊÄÅÊï∞ÁªÑ\r\n    async fetchHouseFitmentStatus() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseFitmentStatusList\");\r\n        this.houseFitmentStatusList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãË£Ö‰øÆÁä∂ÊÄÅÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãÁßüËµÅÊñπÂºèÊï∞ÁªÑ\r\n    async fetchHouseRentalType() {\r\n      try {\r\n        const { data } = await this.$axios.get(\"/house/houseRentalTypeList\");\r\n        this.houseRentalTypeList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÁßüËµÅÊñπÂºèÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    // Êü•ËØ¢ÊàøÂ±ãÁîüÊ¥ªËÆæÊñΩÈÖçÁΩÆÈ°πÊï∞ÁªÑ\r\n    async fetchLivingFacilityType() {\r\n      try {\r\n        const { data } = await this.$axios.get(\r\n          \"/house/houseLivingFacilityList\"\r\n        );\r\n        this.houseLivingFacilityList = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÊàøÂ±ãÁîüÊ¥ªËÆæÊñΩÈÖçÁΩÆÈ°πÊï∞ÁªÑÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    async updateHouse() {\r\n      try {\r\n        this.house.cover = this.cover;\r\n        this.house.detail = this.content;\r\n        this.house.areaId = this.cityAreaId;\r\n        this.house.covers =\r\n          this.coverList.length === 0\r\n            ? null\r\n            : this.coverList.map(entity => entity.url).join(\",\");\r\n        this.house.livingFacilities = JSON.stringify(\r\n          this.houseLivingFacilityList\r\n        );\r\n        const postData = {\r\n          ...this.house,\r\n          typeId: Number(this.house.typeId),\r\n          directionId: Number(this.house.directionId),\r\n          sizedId: Number(this.house.sizedId),\r\n          depositMethodId: Number(this.house.depositMethodId),\r\n          fitmentStatusId: Number(this.house.fitmentStatusId),\r\n          rentalType: Number(this.house.rentalType),\r\n          isSubway: Number(this.house.isSubway)\r\n        };\r\n        const { message } = await this.$axios.put(\"/house/update\", postData);\r\n        this.$notify({\r\n          title: \"ÊàøÂ±ã‰øÆÊîπ\",\r\n          type: \"success\",\r\n          message: message,\r\n          position: \"buttom-right\",\r\n          suration: 1000\r\n        });\r\n        this.toLastPage();\r\n      } catch (error) {\r\n        console.log(\"‰øÆÊîπÊàøÂ±ã‰ø°ÊÅØÂºÇÂ∏∏Ôºö\", error);\r\n        this.$notify({\r\n          title: \"ÊàøÂ±ã‰øÆÊîπ\",\r\n          type: \"info\",\r\n          message: error.message,\r\n          position: \"buttom-right\",\r\n          suration: 1000\r\n        });\r\n      }\r\n    },\r\n    onListener(text) {\r\n      this.content = text;\r\n    },\r\n    async fetchTopArea() {\r\n      try {\r\n        const areaQueryDto = { parentId: 0 };\r\n        const { data } = await this.$axios.post(\"/area/list\", areaQueryDto);\r\n        this.topArea = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÁúÅ‰ªΩ‰ø°ÊÅØÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    async handleAreaChange(topAreaId) {\r\n      try {\r\n        const areaQueryDto = { parentId: topAreaId };\r\n        const { data } = await this.$axios.post(\"/area/list\", areaQueryDto);\r\n        this.cityArea = data;\r\n      } catch (error) {\r\n        console.log(\"Êü•ËØ¢ÁúÅ‰ªΩ‰∏ãÁöÑÂ∏ÇÂå∫‰ø°ÊÅØÂºÇÂ∏∏Ôºö\", error);\r\n      }\r\n    },\r\n    handleCovers(response, file, fileList) {\r\n      this.coverList.push({\r\n        uid: Date.now() + Math.floor(Math.random() * 1000),\r\n        url: response.data\r\n      });\r\n      console.log(\"‰∏ä‰º†ÔºåÊ≠§Êó∂ÁöÑÂõæÁâáÊï∞ÁªÑÔºö\", this.coverList);\r\n    },\r\n    handlePictureCardPreview(file) {\r\n      this.dialogImageUrl = file.url;\r\n      this.dialogVisible = true;\r\n    },\r\n    handleRemove(file, fileList) {\r\n      console.log(\"file:\", file);\r\n\r\n      if (fileList.length === 0) return;\r\n      this.coverList = this.coverList.filter(entity => entity.uid !== file.uid);\r\n    },\r\n    handleImageSuccess(res, file) {\r\n      // ÈÄöÁü•ÊèêÁ§∫\r\n      this.$notify({\r\n        title: \"Â∞ÅÈù¢‰∏ä‰º†\",\r\n        type: res.code === 200 ? \"success\" : \"error\",\r\n        message: res.code === 200 ? \"‰∏ä‰º†ÊàêÂäü\" : res.data,\r\n        position: \"buttom-right\",\r\n        suration: 1000\r\n      });\r\n      if (res.code === 200) {\r\n        this.cover = res.data; // ÂìçÂ∫îÈáåÈù¢ÁöÑdataÔºåÂç≥ÂêéÁ´ØËøîÂõûÁöÑ‰∏ä‰º†ÂêéÁöÑÂõæÁâáÈìæÊé•\r\n      }\r\n    },\r\n    toLastPage() {\r\n      this.$router.go(-1); // ËøîÂõû‰∏ä‰∏ÄÈ°µ\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.living {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  background-color: rgb(246, 247, 248);\r\n  width: 800px;\r\n\r\n  .living-item {\r\n    display: flex;\r\n    padding: 10px;\r\n    gap: 10px;\r\n\r\n    .text {\r\n      font-size: 12px;\r\n    }\r\n  }\r\n}\r\n\r\n.update-area {\r\n  padding-block: 10px;\r\n  background-color: rgb(255, 255, 255);\r\n}\r\n\r\n.area {\r\n  display: flex;\r\n  justify-content: left;\r\n  align-items: center;\r\n  gap: 10px;\r\n  margin-top: 6px;\r\n  margin-block: 30px;\r\n\r\n  span {\r\n    width: 150px;\r\n    display: inline-block;\r\n    text-align: left;\r\n    margin-right: 10px;\r\n    padding: 0 20px;\r\n    font-size: 16px;\r\n    color: rgb(51, 51, 51);\r\n  }\r\n}\r\n\r\n.house-container {\r\n  box-sizing: border-box;\r\n\r\n  .nav {\r\n    margin-bottom: 20px;\r\n  }\r\n}\r\n</style>\r\n"]}]}